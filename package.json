{
  "name": "switch-npm",
  "displayName": "Switch NPM",
  "description": "switch npm",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "switch-npm.switchNpmRegistry",
      "title": "Switch NPM Registry"
    }],
    "menus": {
      "editor/context": [
        {
          "when": "editorTextFocus",
          "command": "switch-npm.switchNpmRegistry",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "pnpm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.16.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
